{
  "//": [
    "SEO: Tooling keeps code style consistent, preventing issues that harm search engine crawlability.",
    "Accessibility: Linting enforces semantic HTML and ARIA patterns flagged by recommended configs.",
    "Performance: Formatter and linters ensure minimal bundle size by avoiding stray assets and whitespace.",
    "Safety: Dependencies are pinned to exact versions to avoid supply-chain surprises."
  ],
  "type": "module",
  "private": true,
  "scripts": {
    "admin": "node admin/server.js",
    "admin:dev": "nodemon admin/server.js",
    "admin:verify": "node admin/verify-installation.js",
    "admin:security": "node admin/verify-security.js",
    "clean": "bundle exec jekyll clean",
    "build:clean": "npm run clean && npm run build",
    "dev:clean": "npm run clean && npm run dev",
    "jekyll": "bundle exec jekyll",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:all": "node scripts/lint-and-parse.js",
    "lint:autofix": "node scripts/lint-parse-autofix.cjs",
    "lint:js": "eslint .",
    "lint:html": "npx htmlhint \"_site/**/*.html\" || true",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "format": "prettier --write .",
    "build:css": "node scripts/build-css.js",
    "watch:css": "sass --load-path=_sass assets/css/main.css:assets/css/main.css --watch --no-source-map",
    "start": "npm run build && npm run serve",
    "dev": "npm run build:css && npm run jekyll -- build && http-server _site -p 4000",
    "dev:watch": "concurrently \"npm run watch:css\" \"npm run jekyll -- build --watch\" \"http-server _site -p 4000 -c-1\"",
    "build": "npm run build:css && npm run jekyll -- build",
    "postbuild": "node scripts/post-build-link.js",
    "serve": "http-server _site -p 4000 -c-1",
    "serve:test": "http-server _site -p 4173 -c-1",
    "build:site": "npm run build",
    "build:full": "npm run build:css && npm run build:logos && npm run jekyll -- build",
    "build:logos": "node scripts/generate-png-logos.js",
    "test": "npm run test:e2e",
    "test:tools": "node --experimental-vm-modules tests/tools/formula-library.test.js",
    "test:e2e": "npm run build && npx playwright test --config=playwright.config.js",
    "test:headed": "npx playwright test --headed",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report",
    "test:debug": "npx playwright test --debug",
    "test:a11y": "npm run build && npx pa11y-ci",
    "test:a11y:single": "npx pa11y http://127.0.0.1:4000",
    "audit:a11y": "npx axe http://127.0.0.1:4000 --exit",
    "verify": "npm run lint && npm run build && npm test",
    "test:contrast": "node scripts/check-contrast.js",
    "scan:contrast": "node scripts/scan-css-contrast.cjs",
    "check:wcag-contrast": "node scripts/check-contrast-wcag.js",
    "audit:compliance": "node scripts/compliance-audit.js",
    "audit": "npm run audit:compliance && npm run check:wcag-contrast",
    "improve": "node scripts/improve-site.cjs",
    "improve:check": "node scripts/improve-site.cjs --check",
    "improve:fix": "node scripts/improve-site.cjs --fix",
    "improve:report": "node scripts/improve-site.cjs --report",
    "deploy": "pwsh -File deploy-to-stone.ps1",
    "deploy:force": "pwsh -File deploy-to-stone.ps1 -Force",
    "deploy:skip-tests": "pwsh -File deploy-to-stone.ps1 -SkipTests",
    "images:webp": "node scripts/convert-images-to-webp.js",
    "slugify": "node scripts/slugify.js",
    "sync:thumbtack": "node scripts/sync_thumbtack_reviews.js",
    "validate": "node scripts/validate-all.js",
    "fix:encoding": "node scripts/fix-encoding.js",
    "check-links": "blc http://127.0.0.1:4000 --recursive --exclude-external"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "cheerio": "1.1.2",
    "clean-css-cli": "^5.6.3",
    "concurrently": "9.2.1",
    "eslint": "8.57.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.6.0",
    "glob": "^13.0.0",
    "html-minifier-terser": "^7.2.0",
    "htmlhint": "1.8.0",
    "http-server": "^14.1.1",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^10.0.1",
    "js-yaml": "4.1.1",
    "pa11y": "^9.0.1",
    "pa11y-ci": "^4.0.1",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "sass": "^1.97.2",
    "sharp": "0.34.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "16.0.0",
    "stylelint-scss": "6.8.1",
    "svgo": "^4.0.0",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=22.19.0"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.8",
    "commander": "^14.0.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "gsap": "^3.14.2",
    "helmet": "^8.0.0",
    "imagemin": "^9.0.1",
    "imagemin-svgo": "^11.0.1",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "lighthouse": "^13.0.1",
    "nodemailer": "^6.9.8",
    "nodemon": "^3.0.2",
    "puppeteer": "^24.35.0",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "swiper": "^12.0.3"
  }
}
